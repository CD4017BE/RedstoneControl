{
	textures = ["rs_ctr:blocks/rs_port0"];
}

main(t) {
	offsetUV(2 * (t % 8), 5 * #(t / 8));
	Loc Q = [7,7,5, 9,9,8]#, T = [0,0, 2,3]#;
	for(F:["u-V","u+V","-uV","+uV"]) {
		add(rect(Q, T, F, 0));
	}
	add(rect(Q, [0,3, 2,5]#, "uv-", 0));
}

wire(t) {main(t);}
wireless(t) {main(t + 3);}
misc(t) {main(t + 6);}
